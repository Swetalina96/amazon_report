\"amazon_india_2019.csv\",\n","    \"amazon_india_2020.csv\",\n","    \"amazon_india_2021.csv\",\n","    \"amazon_india_2022.csv\",\n","    \"amazon_india_2023.csv\",\n","    \"amazon_india_2024.csv\",\n","    \"amazon_india_2025.csv\"\n","]\n","\n","dfs = []\n","\n","for file in files:\n","    temp_df = pd.read_csv(file, low_memory=False)\n","\n","    # Check available columns\n","    if 'date' in temp_df.columns:\n","        temp_df['date'] = pd.to_datetime(temp_df['date'], errors='coerce')\n","    elif 'Date' in temp_df.columns:  # handle possible capitalization\n","        temp_df['Date'] = pd.to_datetime(temp_df['Date'], errors='coerce')\n","        temp_df.rename(columns={'Date': 'date'}, inplace=True)\n","    else:\n","        # If no date column exists, just continue\n","        temp_df['date'] = pd.NaT\n","\n","    dfs.append(temp_df)\n","\n","# Merge all DataFrames\n","merged_df = pd.concat(dfs, ignore_index=True)\n","\n","# Remove duplicates\n","merged_df.drop_duplicates(inplace=True)\n","\n","# Reset index\n","merged_df.reset_index(drop=True, inplace=True)\n","\n","# Save merged CSV\n","merged_df.to_csv(\"amazon_india_2015_2025_merged.csv\", index=False)\n","\n","print(\"✅ Merged CSV created successfully!\")\n","print(\"Merged data shape:\", merged_df.shape)\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"m1mqrGvSkSuX","executionInfo":{"status":"ok","timestamp":1757272486338,"user_tz":-330,"elapsed":38635,"user":{"displayName":"pradeepa Gnanasekaran","userId":"07445249332452681846"}},"outputId":"dc86f97f-75e2-4cde-ffba-2992f2b6688b"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["✅ Merged CSV created successfully!\n","Merged data shape: (1127609, 35)\n"]}]},{"cell_type":"markdown","source":["Dashboard Development\n","\n","Executive Dashboard\n","\n","\n","\n","\n","\n"],"metadata":{"id":"QN4HS5-5a_fh"}},{"cell_type":"code","source":["# amazon_dashboards.py\n","import streamlit as st\n","import pandas as pd\n","import numpy as np\n","import plotly.express as px\n","from prophet import Prophet\n","\n","# This script is designed to be run as a Streamlit application.\n","# Save this code as a .py file (e.g., amazon_dashboards.py) and run from your terminal:\n","# streamlit run amazon_dashboards.py\n","\n","# ------------------------------\n","# Load Data\n","# ------------------------------\n","# Ensure the data path is correct for your environment\n","DATA_PATH = \"amazon_india_2015_2025_merged.csv\" # Update if your merged file has a different name or location\n","\n","try:\n","    df = pd.read_csv(DATA_PATH, low_memory=False)\n","    # Convert date column, trying common names\n","    date_col = None\n","    for col in df.columns:\n","        if 'date' in col.lower():\n","            date_col = col\n","            break\n","\n","    if date_col:\n","        df['order_date'] = pd.to_datetime(df[date_col], errors='coerce')\n","        df.dropna(subset=['order_date'], inplace=True) # Drop rows with invalid dates\n","        df['year'] = df['order_date'].dt.year\n","        df['month'] = df['order_date'].dt.month\n","    else:\n","        st.error(\"Could not find a date column in the dataset. Please ensure one exists.\")\n","        st.stop() # Stop execution if no date column is found\n","\n","except FileNotFoundError:\n","    st.error(f\"Data file not found at {DATA_PATH}. Please check the file path.\")\n","    st.stop() # Stop execution if file is not found\n","except Exception as e:\n","    st.error(f\"An error occurred while loading or processing the data: {e}\")\n","    st.stop() # Stop execution for other data errors\n","\n","\n","# ------------------------------\n","# Dashboard Filters\n","# ------------------------------\n","st.sidebar.header(\"Filters\")\n","\n","# Use unique years and months from the data\n","if 'year' in df.columns and 'month' in df.columns:\n","    year_filter = st.sidebar.selectbox(\"Select Year\", sorted(df['year'].unique()))\n","    month_filter = st.sidebar.selectbox(\"Select Month\", sorted(df['month'].unique()))\n","\n","    # ------------------------------\n","    # Apply Filters\n","    # ------------------------------\n","    if year_filter and month_filter: # Check if filters have been selected\n","        df_filtered = df[(df['year'] == year_filter) & (df['month'] == month_filter)].copy() # Use .copy() to avoid SettingWithCopyWarning\n","\n","        # ------------------------------\n","        # Display Metrics\n","        # ------------------------------\n","        st.subheader(f\"Metrics for {year_filter}-{month_filter:02d}\")\n","\n","        if not df_filtered.empty:\n","            # Ensure the revenue column exists\n","            revenue_col = None\n","            for col in df_filtered.columns:\n","                if 'amount' in col.lower() or 'price' in col.lower():\n","                     revenue_col = col\n","                     break\n","\n","            if revenue_col:\n","                # Ensure the revenue column is numeric\n","                df_filtered[revenue_col] = pd.to_numeric(df_filtered[revenue_col], errors='coerce')\n","                df_filtered.dropna(subset=[revenue_col], inplace=True) # Drop rows with non-numeric revenue\n","\n","                total_revenue = df_filtered[revenue_col].sum()\n","                total_orders = len(df_filtered)\n","                unique_customers = df_filtered['customer_id'].nunique() if 'customer_id' in df_filtered.columns else 0\n","\n","                col1, col2, col3 = st.columns(3)\n","                col1.metric(\"Total Revenue\", f\"₹{total_revenue:,.2f}\")\n","                col2.metric(\"Total Orders\", total_orders)\n","                col3.metric(\"Unique Customers\", unique_customers)\n","\n","                # ------------------------------\n","                # Example Plot (Daily Revenue Trend)\n","                # ------------------------------\n","                st.subheader(\"Revenue Trend (Filtered Month)\")\n","                # Group by date for daily trend\n","                daily_revenue = df_filtered.groupby(pd.Grouper(key='order_date', freq='D'))[revenue_col].sum().reset_index()\n","                fig = px.line(daily_revenue, x='order_date', y=revenue_col, title=f\"Daily Revenue in {year_filter}-{month_filter:02d}\")\n","                st.plotly_chart(fig, use_container_width=True)\n","\n","            else:\n","                st.warning(\"Could not find a suitable revenue column in the dataset.\")\n","        else:\n","            st.info(\"No data available for the selected year and month.\")\n","    else:\n","        st.info(\"Please select a Year and Month from the filters.\")\n","\n","else:\n","    st.warning(\"Year or Month column not found after date processing.\")\n","\n","\n","# ------------------------------\n","# Add more dashboard elements here based on your analysis questions (Q1-Q20)\n","# For example, you could add sections for:\n","# - Yearly Revenue Trend\n","# - Monthly Sales Heatmap\n","# - RFM Analysis\n","# - Payment Method Evolution\n","# - Category Performance\n","# - Geographic Analysis\n","# - Festival Sales Impact\n","# - Customer Age Group Analysis\n","# - Price vs Demand Analysis\n","# - Delivery Performance\n","# - Return Analysis\n","# - Brand Performance\n","# - CLV Analysis\n","# - Discount Effectiveness\n","# - Product Rating Analysis\n","# - Customer Journey Analysis\n","# - Inventory & Product Lifecycle\n","# - Competitive Pricing\n","# - Business Health Summary\n","# ------------------------------\n","\n","# Example: Add a section for Yearly Revenue Trend (assuming df has all years)\n","if 'year' in df.columns and revenue_col: # Check if year and revenue_col exist in the full df\n","    st.subheader(\"Yearly Revenue Trend\")\n","    yearly_revenue = df.groupby('year')[revenue_col].sum().reset_index()\n","    fig_yearly = px.line(yearly_revenue, x='year', y=revenue_col, title=\"Yearly Revenue Trend (All Data)\")\n","    st.plotly_chart(fig_yearly, use_container_width=True)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"BW2lN_bLnG9p","executionInfo":{"status":"ok","timestamp":1757272885859,"user_tz":-330,"elapsed":81196,"user":{"displayName":"pradeepa Gnanasekaran","userId":"07445249332452681846"}},"outputId":"872fbb20-d2d3-4b10-ea8b-5a6c990e5a65"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stderr","text":["2025-09-07 19:20:19.415 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.416 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.417 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.427 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.428 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.429 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.430 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.431 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.433 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.434 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.442 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare mode.\n","2025-09-07 19:20:19.443 Thread 'MainThread': missing ScriptRunContext! This warning can be ignored when running in bare