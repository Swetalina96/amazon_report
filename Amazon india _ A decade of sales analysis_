\" \", f)\n","\n","# If you've mounted Google Drive, also list the project folder (uncomment if used)\n","# from google.colab import drive\n","# drive.mount('/content/drive')\n","# print(\"Files in Drive project folder:\")\n","# for f in sorted(glob.glob('/content/drive/MyDrive/**/amazon*', recursive=True))[:50]:\n","#     print(\" \", f)\n","\n","# 2. Try to automatically discover CSV files (several patterns)\n","candidates = []\n","patterns = [\n","    \"/content/amazon_india_*.csv\",\n","    \"/content/amazon*.csv\",\n","    \"/content/*amazon*.csv\",\n","    \"/content/drive/MyDrive/**/amazon_india_*.csv\",\n","    \"/content/drive/MyDrive/**/amazon*.csv\",\n","]\n","for pat in patterns:\n","    found = glob.glob(pat, recursive=True)\n","    if found:\n","        candidates.extend(found)\n","\n","candidates = sorted(list(set(candidates)))\n","print(\"\\nDiscovered CSV files:\")\n","for f in candidates:\n","    print(\"  \", f)\n","\n","if not candidates:\n","    raise FileNotFoundError(\"No CSV files discovered. Please upload your csvs to Colab (/content) or mount Google Drive and adjust the patterns above.\")\n","\n","# 3. Read all discovered CSVs safely and combine\n","import pandas as pd\n","dfs = []\n","for f in candidates:\n","    try:\n","        print(\"Reading:\", f)\n","        tmp = pd.read_csv(f, low_memory=False)\n","        tmp['_source_file'] = os.path.basename(f)\n","        dfs.append(tmp)\n","    except Exception as e:\n","        print(\"  Failed to read\", f, \":\", e)\n","\n","if not dfs:\n","    raise RuntimeError(\"No dataframes could be read from the discovered CSV files.\")\n","\n","df_all = pd.concat(dfs, ignore_index=True)\n","print(\"\\nCombined dataframe shape:\", df_all.shape)\n","print(\"Columns:\", df_all.columns.tolist()[:40])\n","\n","# 4. Paste the flexible cleaning pipeline from earlier (all 10 tasks)\n","import re, numpy as np\n","\n","def find_col(df, keywords):\n","    kws = [k.lower() for k in keywords]\n","    for col in df.columns:\n","        low = col.lower()\n","        for k in kws:\n","            if k in low:\n","                return col\n","    return None\n","\n","def standardize_dates(df):\n","    date_col = find_col(df, ['date', 'order_date', 'placed_on', 'purchase_date'])\n","    if date_col is None:\n","        df['order_date_clean'] = pd.NaT\n","        df['order_date_invalid'] = False\n","        return df\n","    parsed = pd.to_datetime(df[date_col], errors='coerce', dayfirst=True, infer_datetime_format=True)\n","    rem = parsed.isna() & df[date_col].notna()\n","    if rem.any():\n","        parsed.loc[rem] = pd.to_datetime(df.loc[rem, date_col], errors='coerce', dayfirst=False, infer_datetime_format=True)\n","    df['order_date_clean'] = pd.to_datetime(parsed).dt.date\n","    df['order_date_invalid'] = df['order_date_clean'].isna() & df[date_col].notna()\n","    return df\n","\n","def clean_price(df):\n","    price_col = find_col(df, ['price', 'amount', 'mrp', 'selling_price', 'list_price', 'price_inr', 'original_price'])\n","    if price_col is None:\n","        df['unit_price_clean'] = np.nan\n","        return df\n","    s = df[price_col].astype(str).fillna('')\n","    cleaned = s.str.replace(r'[^\\d\\.\\-]', '', regex=True)\n","    cleaned = cleaned.replace('', np.nan)\n","    df['unit_price_clean'] = pd.to_numeric(cleaned, errors='coerce')\n","    return df\n","\n","def clean_ratings(df):\n","    rating_col = find_col(df, ['rating', 'customer_rating', 'review_score', 'stars'])\n","    if rating_col is None:\n","        df['rating_numeric'] = np.nan\n","        return df\n","    s = df[rating_col].astype(str).fillna('')\n","    def parse_rating(x):\n","        x = x.strip().lower()\n","        if x == '' or x in ['nan','none','na']: return np.nan\n","        frac = re.match(r'^\\s*([0-9]+(?:\\.[0-9]+)?)\\s*/\\s*([0-9]+(?:\\.[0-9]+)?)', x)\n","        if frac:\n","            num = float(frac.group(1)); den = float(frac.group(2))\n","            if den == 0: return np.nan\n","            return (num/den)*5.0 if den != 5 else num\n","        m = re.match(r'^([0-9]+(?:\\.[0-9]+)?)', x)\n","        if m:\n","            val = float(m.group(1))\n","            if val > 5:\n","                if val <= 10:\n","                    return min(val,5.0)\n","                if val/10 <= 5:\n","                    return val/10\n","                return np.nan\n","            return val\n","        return np.nan\n","    df['rating_numeric'] = s.map(parse_rating).astype(float)\n","    df['rating_numeric'] = df['rating_numeric'].where(df['rating_numeric'].between(1.0,5.0), np.nan)\n","    return df\n","\n","def normalize_city(df):\n","    city_col = find_col(df, ['city', 'customer_city', 'shipping_city', 'delivery_city'])\n","    if city_col is None:\n","        df['city_clean'] = np.nan\n","        return df\n","    s = df[city_col].astype(str).fillna('').str.strip()\n","    mapping = {\n","        'bangalore': 'Bengaluru', 'bengaluru': 'Bengaluru', 'banglore': 'Bengaluru',\n","        'mumbai': 'Mumbai', 'bombay': 'Mumbai',\n","        'delhi': 'New Delhi', 'new delhi': 'New Delhi',\n","        'chennai': 'Chennai', 'madras': 'Chennai',\n","        'kolkata': 'Kolkata', 'calcutta': 'Kolkata',\n","        'hyderabad': 'Hyderabad', 'secunderabad': 'Hyderabad'\n","    }\n","    def map_city(x):\n","        if not x or x.strip()=='nan': return np.nan\n","        key = re.split(r'[/,-]', x.lower())[0].strip()\n","        key = key.replace('.', '').replace(\"'\", '').strip()\n","        return mapping.get(key, key.title())\n","    df['city_clean'] = s.map(map_city)\n","    return df\n","\n","def normalize_booleans(df):\n","    bool_like = [c for c in df.columns if any(k in c.lower() for k in ['prime','is_prime','festival','is_festival','eligible','is_eligible','is_active'])]\n","    true_set = set(['true','t','1','y','yes'])\n","    false_set = set(['false','f','0','n','no'])\n","    for col in bool_like:\n","        s = df[col].astype(str).str.strip().str.lower().fillna('')\n","        def to_bool(x):\n","            if x in true_set: return True\n","            if x in false_set: return False\n","            try:\n","                v = float(x); return bool(v)\n","            except: return np.nan\n","        df[col+'_clean'] = s.map(to_bool)\n","    return df\n","\n","def normalize_category(df):\n","    cat_col = find_col(df, ['category', 'product_category', 'cat'])\n","    if cat_col is None:\n","        df['category_clean'] = np.nan\n","        return df\n","    s = df[cat_col].astype(str).fillna('').str.lower()\n","    mapping = {\n","        'electronics': 'Electronics', 'electronic': 'Electronics',\n","        'electronics & accessories': 'Electronics', 'mobiles & accessories': 'Mobiles & Accessories',\n","        'home & kitchen': 'Home & Kitchen', 'fashion': 'Fashion',\n","        'clothing': 'Fashion', 'books': 'Books', 'beauty': 'Beauty & Personal Care'\n","    }\n","    def map_cat(x):\n","        key = x.replace('&','and').strip()\n","        for k in mapping:\n","            if k in key:\n","                return mapping[k]\n","        return x.title() if x else np.nan\n","    df['category_clean'] = s.map(map_cat)\n","    return df\n","\n","def clean_delivery_days(df):\n","    col = find_col(df, ['delivery_days', 'delivery_time', 'days_to_deliver', 'delivery_duration'])\n","    if col is None:\n","        df['delivery_days_clean'] = np.nan\n","        return df\n","    s = df[col].astype(str).fillna('').str.lower().str.strip()\n","    def parse_days(x):\n","        if x=='' or x in ['nan','none']: return np.nan\n","        if 'same' in x: return 1.0\n","        m = re.match(r'^\\s*(\\d+)\\s*[-–to]\\s*(\\d+)', x)\n","        if m: return (float(m.group(1))+float(m.group(2)))/2.0\n","        m2 = re.match(r'^\\s*(\\d+)', x)\n","        if m2: return float(m2.group(1))\n","        return np.nan\n","    df['delivery_days_clean'] = s.map(parse_days)\n","    df.loc[df['delivery_days_clean'] < 0, 'delivery_days_clean'] = np.nan\n","    df.loc[df['delivery_days_clean'] > 30, 'delivery_days_clean'] = np.nan\n","    return df\n","\n","def handle_duplicates(df):\n","    keys = []\n","    for k in ['customer_id','customer','user_id','buyer_id']:\n","        if k in df.columns:\n","            keys.append(k); break\n","    for k in ['product_id','product','sku','item_id']:\n","        if k in df.columns:\n","            keys.append(k); break\n","    if 'order_date_clean' in df.columns: keys.append('order_date_clean')\n","    elif 'order_date' in df.columns: keys.append('order_date')\n","    if 'total_amount' in df.columns: keys.append('total_amount')\n","    elif 'unit_price_clean' in df.columns: keys.append('unit_price_clean')\n","    keys = [k for k in keys if k in df.columns]\n","    if not keys:\n","        df['is_potential_duplicate'] = False\n","        return df\n","    df['is_exact_duplicate'] = df.duplicated(subset=keys, keep=False)\n","    df['dup_reason'] = None\n","    for idx in df[df['is_exact_duplicate']].index:\n","        qty = df.at[idx, 'qty'] if 'qty' in df.columns else None\n","        if qty is not None and pd.notna(qty) and qty>1:\n","            df.at[idx, 'dup_reason'] = 'bulk_order_like'\n","        else:\n","            df.at[idx, 'dup_reason'] = 'potential_error'\n","    return df\n","\n","def correct_price_outliers(df):\n","    price_col = 'unit_price_clean' if 'unit_price_clean' in df.columns else find_col(df, ['price','amount'])\n","    cat_col = 'category_clean' if 'category_clean' in df.columns else find_col(df, ['category','cat'])\n","    if price_col is None:\n","        return df\n","    df['price_outlier_flag'] = False\n","    if cat_col and cat_col in df.columns:\n","        for cat, g in df.groupby(cat_col):\n","            prices = g[price_col].dropna()\n","            if len(prices) < 5: continue\n","            med = prices.median()\n","            if med == 0: continue\n","            high_mask = (df[price_col] > med * 100) & (df[cat_col] == cat)\n","            if high_mask.any():\n","                original = df.loc[high_mask, price_col]\n","                corrected_by_100 = original / 100.0\n","                corrected_by_10  = original / 10.0\n","                choose100 = (abs(corrected_by_100 - med) < abs(corrected_by_10 - med))\n","                # apply corrections\n","                idx100 = original.index[choose100.values] if choose100.any() else []\n","                idx10  = original.index[~choose100.values] if (~choose100).any() else []\n","                if len(idx100)>0: df.loc[idx100, price_col] = df.loc[idx100, price_col] / 100.0\n","                if len(idx10)>0:  df.loc[idx10, price_col]  = df.loc[idx10, price_col] / 10.0\n","                df.loc[high_mask, 'price_outlier_flag'] = True\n","    else:\n","        prices = df[price_col].dropna()\n","        med = prices.median() if len(prices)>0 else 0\n","        high_mask = df[price_col] > med * 100\n","        if high_mask.any():\n","            df.loc[high_mask, price_col] = df.loc[high_mask, price_col] / 100.0\n","            df.loc[high_mask, 'price_outlier_flag'] = True\n","    return df\n","\n","def standardize_payments(df):\n","    pay_col = find_col(df, ['payment', 'payment_method', 'payment_type', 'pay_method'])\n","    if pay_col is None:\n","        df['payment_clean'] = np.nan\n","        return df\n","    s = df[pay_col].astype(str).fillna('').str.lower()\n","    def map_pay(x):\n","        if x=='' or x in ['nan','none']: return np.nan\n","        if any(k in x for k in ['upi','phonepe','google','gpay','paytm','bhim']): return 'UPI'\n","        if any(k in x for k in ['credit', 'cc', 'card', 'visa', 'mastercard', 'amex']): return 'Credit Card'\n","        if any(k in x for k in ['debit']): return 'Debit Card'\n","        if any(k in x for k in ['cash on delivery','cod','c.o.d']): return 'COD'\n","        if any(k in x for k in ['wallet','mobikwik','freecharge','amazon pay','amazonpay']): return 'Wallet'\n","        if 'netbank' in x or 'net banking' in x: return 'NetBanking'\n","        return x.title()\n","    df['payment_clean'] = s.map(map_pay)\n","    return df\n","\n","# Apply\n","df_all = standardize_dates(df_all)\n","df_all = clean_price(df_all)\n","df_all = clean_ratings(df_all)\n","df_all = normalize_city(df_all)\n","df_all = normalize_booleans(df_all)\n","df_all = normalize_category(df_all)\n","df_all = clean_delivery_days(df_all)\n","df_all = handle_duplicates(df_all)\n","df_all = correct_price_outliers(df_all)\n","df_all = standardize_payments(df_all)\n","\n","# Save and show summary\n","out_path = '/content/cleaned_sales_full.csv'\n","df_all.to_csv(out_path, index=False)\n","print(\"\\nSaved cleaned file to:\", out_path)\n","print(\"Columns now include examples:\", [c for c in df_all.columns if c.endswith('_clean') or 'rating' in c or 'price' in c][:50])\n","display(df_all.head(5))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"NAKhMVO1S_H1","executionInfo":{"status":"ok","timestamp":1757267460904,"user_tz":-330,"elapsed":73128,"user":{"displayName":"pradeepa Gnanasekaran","userId":"07445249332452681846"}},"outputId":"2f87cb5d-9cef-48be-db1a-ec33f49bd3a8"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Files in /content (top-level):\n","  .config\n","  amazon_india_2015.csv\n","  amazon_india_2016.csv\n","  amazon_india_2017.csv\n","  amazon_india_2018.csv\n","  amazon_india_2019.csv\n","  amazon_india_2020.csv\n","  amazon_india_2021.csv\n","  amazon_india_2022.csv\n","  amazon_india_2023.csv\n","  amazon_india_2024.csv\n","  amazon_india_2025.csv\n","  amazon_sales.db\n","  sample_data\n","\n","Discovered CSV files:\n","   /content/amazon_india_2015.csv\n","   /content/amazon_india_2016.csv\n","   /content/amazon_india_2017.csv\n","   /content/amazon_india_2018.csv\n","   /content/amazon_india_2019.csv\n","   /content/amazon_india_2020.csv\n","   /content/